    },
    "outputs": {
      "k": "K line (fast stochastic)",
      "d": "D line (slow stochastic)"
    },
    "supports_cross": true,
    "timeframes": ["M1", "M5", "M15", "M30", "H1", "H4", "D1", "W1"]
  },
  {
    "name": "Bollinger",
    "full_name": "Bollinger Bands",
    "description": "Volatility indicator with upper/lower bands at N standard deviations from SMA. Price touching upper band = potential overbought, lower band = potential oversold. Band squeeze = low volatility, expansion = high volatility.",
    "params": {
      "period": {"type": "int", "default": 20, "description": "SMA period"},
      "deviation": {"type": "float", "default": 2.0, "description": "Standard deviation multiplier"}
    },
    "outputs": {
      "upper": "Upper band",
      "middle": "Middle band (SMA)",
      "lower": "Lower band"
    },
    "supports_cross": true,
    "comparable_to": ["price"],
    "timeframes": ["M1", "M5", "M15", "M30", "H1", "H4", "D1", "W1"]
  },
  {
    "name": "ATR",
